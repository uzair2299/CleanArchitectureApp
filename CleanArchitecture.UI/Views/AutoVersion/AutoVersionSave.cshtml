    <div class="kt-container  kt-container--fluid  kt-grid__item kt-grid__item--fluid">
        <div class="kt-portlet kt-portlet--mobile">
            <div class="kt-portlet__head kt-portlet__head--lg" style="background-color: #f9f9fc;">
                <div class="kt-portlet__head-label">
                    <span class="kt-portlet__head-icon">
                        <i class="fab fa-whmcs"></i>
                    </span>
                    <h3 class="kt-portlet__head-title">
                        Auto Specfication
                    </h3>
                </div>
            </div>
            <div class="kt-portlet__body">
                <form class="kt-form kt-form--label-right" id="AutoVersion" asp-action="AutoVersionSave" asp-controller="AutoVersion" method="post" onsubmit=" return autoVersion.validdateAutoVersion();" enctype="multipart/form-data">
                    <div class="modal-body">
                        <div class="kt-portlet__body">
                            <div class="row">
                                <div class="col-12">
                                    <div class="kt-portlet kt-portlet--bordered">
                                        <div class="kt-portlet__head bg-dark">
                                            <div class="kt-portlet__head-label">
                                                <h4 class="kt-portlet__head-title text-white">
                                                    General Info
                                                </h4>
                                            </div>
                                        </div>
                                        <div class="kt-portlet__body">
                                            <div class="kt-widget1 p-0">
                                                <div class="form-group row">
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="SelectedManufacturer"><span class="text-danger">*</span> Auto Manufactuer:</label>
                                                        @*@Html.DropDownListFor(x => Version.SelectedManufacturer,Version.AutoManufacturerLookup, htmlAttributes: new { @class = "form-control" })*@
                                                        <select asp-for="SelectedManufacturer" asp-items="Model.AutoManufacturerLookup" class="form-control kt-select2" name="SelectedManufacturer" style="width:100%" onchange="autoVersion.getAutoModelLookup(this);" id="SelectedManufacturer">
                                                            <option value="">Select Manufacturer</option>
                                                        </select>
                                                        <div class="invalid-feedback"></div>
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="SelectedManufacturer"><span class="text-danger">*</span> Auto Model:</label>
                                                        @*@Html.DropDownListFor(x => Version.SelectedManufacturer,Version.AutoManufacturerLookup, htmlAttributes: new { @class = "form-control" })*@
