                                                <div class="form-group row">
                                                    <div class="col-lg-4">
                                                        <label class="form-control-label" for="SelectedStabilityControlSystem"><span class="text-danger">*</span> SelectedStability Control System:</label>
                                                        <select asp-for="SelectedStabilityControlSystem" asp-items="Model.StabilityControlSystem" class="form-control kt-select2" name="SelectedStabilityControlSystem" style="width:100%" id="SelectedStabilityControlSystem">
                                                            <option value="">Select SelectedStability Control System</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
