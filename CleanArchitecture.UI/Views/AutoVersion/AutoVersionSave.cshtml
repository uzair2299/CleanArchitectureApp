                                                            <option value="">Select Model</option>
                                                        </select>
                                                        <div class="invalid-feedback"></div>
                                                    </div>


                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="AutoVersionName"><span class="text-danger">*</span> Version Name:</label>
                                                        <input type="text" name="AutoVersionName" class="form-control" placeholder="" asp-for="AutoVersionName">
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="SelectedManufacturer"><span class="text-danger">*</span>Body Type:</label>
                                                        <select asp-for="SelectedBodyType" asp-items="Model.AutoBodyTypeLookup" class="form-control kt-select2" name="SelectedBodyType" style="width:100%"  id="SelectedBodyType" onchange="AutoSolutionUtility.onCangeValidation(this)">
                                                            <option value="">Select Body Type</option>
                                                        </select>
                                                        <div class="invalid-feedback"></div>
                                                    </div>

                                                </div>
                                                <div class="form-group row">
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="CurrentPrice"><span class="text-danger">*</span> Price:</label>
                                                        <input name="CurrentPrice" class="form-control" placeholder="Enter Price" asp-for="CurrentPrice">
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label"><span class="text-danger">*</span>Start Date</label>
                                                        <input type="month" class="form-control startMonth" placeholder="Select Date" value="" id="startMonth">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-12">
                                    <div class="kt-portlet kt-portlet--bordered">
                                        <div class="kt-portlet__head bg-dark">
                                            <div class="kt-portlet__head-label">
                                                <h4 class="kt-portlet__head-title text-white">
                                                    Mechanical/Performance
                                                </h4>
                                            </div>
                                        </div>
                                        <div class="kt-portlet__body">
                                            <div class="kt-widget1 p-0">
                                                <div class="form-group row">
                                                    <div class="col-lg-4">
                                                        <label class="form-control-label" for="SelectedTransmissionType"><span class="text-danger">*</span> Transmission Type:</label>
                                                        @*@Html.DropDownListFor(x => Version.SelectedManufacturer,Version.AutoManufacturerLookup, htmlAttributes: new { @class = "form-control" })*@
                                                        <select asp-for="SelectedTransmissionType" asp-items="Model.TransmissionType" class="form-control kt-select2" name="SelectedTransmissionType" style="width:100%" onchange="autoVersion.getAutoModelLookup(this);" id="SelectedTransmissionType">
                                                            <option value="">Select TransmissionType</option>
                                                        </select>
                                                    </div>
                                                    
                                                </div>
                                                <div class="form-group row">
                                                    
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-12">
                                    <div class="kt-portlet kt-portlet--bordered">
                                        <div class="kt-portlet__head bg-dark">
                                            <div class="kt-portlet__head-label">
                                                <h4 class="kt-portlet__head-title text-white">
                                                    Dimensions
                                                </h4>
                                            </div>
                                        </div>
                                        <div class="kt-portlet__body">
                                            <div class="kt-widget1 p-0">
                                                <div class="form-group row">
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="ExteriorHeight">Exterior Height(mm):</label>
                                                        <input name="ExteriorHeight" class="form-control" placeholder="" asp-for="ExteriorHeight" value="">
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="ExteriorWidth">Exterior Width(mm):</label>
                                                        <input name="ExteriorWidth" class="form-control" placeholder="" asp-for="ExteriorWidth" value="">
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="ExteriorLength">Exterior Length(mm):</label>
                                                        <input name="ExteriorLength" class="form-control" placeholder="" asp-for="ExteriorLength">
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="InteriorHeight">Interior Height(mm):</label>
                                                        <input name="InteriorHeight" class="form-control" placeholder="" asp-for="InteriorHeight">
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="InteriorWidth">Interior Width(mm):</label>
                                                        <input name="InteriorWidth" class="form-control" placeholder="" asp-for="InteriorWidth" value="">
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="InteriorLength">Interior Length(mm):</label>
                                                        <input name="InteriorLength" class="form-control" placeholder="" asp-for="InteriorLength" value="">
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="TreadFront">Tread Front(mm):</label>
                                                        <input name="TreadFront" class="form-control" placeholder="" asp-for="TreadFront" value="">
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="TreadRear">Tread Rear(mm):</label>
                                                        <input name="TreadRear" class="form-control" placeholder="" asp-for="TreadRear" value="">
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="OverhangFront">Overhang Front(mm):</label>
                                                        <input name="OverhangFront" class="form-control" placeholder="" asp-for="OverhangFront" value="">
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="OverhangRear">Overhang Rear(mm):</label>
                                                        <input name="OverhangRear" class="form-control" placeholder="" asp-for="OverhangRear" value="">
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="Wheelbase">Wheelbase(mm):</label>
                                                        <input name="Wheelbase" class="form-control" placeholder="" asp-for="Wheelbase" value="">
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="GroundClearance">Ground Clearance(mm):</label>
                                                        <input name="GroundClearance" class="form-control" placeholder="" asp-for="GroundClearance" value="">
                                                    </div>

                                                </div>
                                                <div class="form-group row">
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="RunningGroundClearance">Running Ground Clearance:</label>
                                                        <input type="text" name="RunningGroundClearance" class="form-control" placeholder="" asp-for="RunningGroundClearance" value="">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-12">
                                    <div class="kt-portlet kt-portlet--bordered">
                                        <div class="kt-portlet__head bg-dark">
                                            <div class="kt-portlet__head-label">
                                                <h4 class="kt-portlet__head-title text-white">
                                                    Weight & Capacity
                                                </h4>
                                            </div>
                                        </div>
                                        <div class="kt-portlet__body">
                                            <div class="kt-widget1 p-0">
                                                <div class="form-group row">
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="ExteriorHeight">Seating Capacity(Persons):</label>
                                                        <input step="any" name="SeatingCapacity" class="form-control" placeholder="" asp-for="SeatingCapacity" value="" onkeypress="isNumberKey(event)">
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="ExteriorWidth">Fuel Tank Capacity(Liters):</label>
                                                        <input name="FuelTankCapacity" class="form-control" placeholder="" asp-for="FuelTankCapacity" value="">
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label class="form-control-label" for="CrubWeight">Crub Weight(Kg):</label>
                                                        <input name="CrubWeight" class="form-control" placeholder="" asp-for="CrubWeight" value="">
